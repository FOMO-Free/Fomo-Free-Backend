{
  "name": "larson_group_planner_backend",
  "version": "1.0.0",
  "description": "Backend for accessing Group Planner API",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "migrate": "knex migrate:latest",
    "rollback": "knex migrate:rollback",
    "seed": "knex seed:run",
    "resetdb": "npm run rollback && npm run migrate",
    "migrateh": "heroku run knex migrate:latest -a fomo-free",
    "rollbackh": "heroku run knex migrate:rollback -a fomo-free",
    "databaseh": "heroku pg:psql -a fomo-free",
    "seedh": "heroku run knex seed:run -a fomo-free"
  },
  "engines": {
    "node": "14.16.0",
    "npm": "6.14.11"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "2.8.5",
    "dotenv": "10.0.0",
    "express": "^4.17.1",
    "helmet": "4.6.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "0.95.7",
    "knex-cleaner": "1.3.1",
    "node": "^16.5.0",
    "node-cron": "^3.0.0",
    "pg": "8.6.0"
  },
  "devDependencies": {
    "@types/jest": "26.0.24",
    "cross-env": "7.0.3",
    "eslint": "7.31.0",
    "eslint-plugin-react": "^7.28.0",
    "jest": "27.0.6",
    "jwt-decode": "^3.1.2",
    "nodemon": "2.0.12",
    "supertest": "6.1.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/loshjarson/Larson_Group_Planner_Backend.git"
  },
